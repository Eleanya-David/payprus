   document.querySelector('.form-status').innerHTML += message;\n                });\n            }\n\n            function removeValidationError() {\n                let errors = document.querySelectorAll('.error-message');\n                let formStatus = document.querySelector('.form-status')\n                errors.forEach(function (error) {\n                    error.previousSibling.classList.remove('input-error');\n                    error.previousSibling.removeAttribute('aria-invalid');\n                    error.previousSibling.removeAttribute('aria-describedby');\n                    error.remove();\n                });\n\n                if (formStatus) {\n                    document.querySelector('.form-status').remove();\n                }\n            }\n\n            const validateEmail = (email) => {\n                return String(email)\n                    .toLowerCase()\n                    .match(\n                        \/^(([^<>()[\\]\\\\.,;:\\s@\"]+(\\.[^<>()[\\]\\\\.,;:\\s@\"]+)*)|(\".+\"))@((\\[[0-9]{1,3}\\.[0-9]{1,3}\\.[0-9]{1,3}\\.[0-9]{1,3}\\])|(([a-zA-Z\\-0-9]+\\.)+[a-zA-Z]{2,}))$\/\n                    );\n            };\n\n            function subscribe(data) {\n                data.site_id = siteId;\n                data.email_capture_id = emailCaptureId;\n                data.page_path = pagePath;\n                data.nonce = nonce;\n                \n                        let xhr = new XMLHttpRequest();\n                        xhr.open(\"POST\", \"\/wp-json\/subscribe\/email\");\n\n                        xhr.setRequestHeader(\"Accept\", \"application\/json\");\n                        xhr.setRequestHeader(\"Content-Type\", \"application\/json\");\n\n                        xhr.onload = () => handleResponse(xhr.responseText);\n\n                        xhr.send(JSON.stringify(data));\n\n                                    }\n\n            function handleResponse(responseString) {\n                const response = JSON.parse(responseString);\n                if (response.code === 409) {\n                    let email = document.querySelector(`#email-${widgetId}`);\n                    showValidationErrors(email, response.message);\n                    groupErrorMessages();\n                    document.querySelector('.agp-subscription-form .input-error').focus();\n\n                    return;\n                }\n                if (typeof(DY) !== \"undefined\" && typeof(DY.API) !== \"undefined\") {\n                    DY.API(\"event\", {\n                        name: \"Newsletter Subscription\",\n                        properties: {\n                            dyType: \"newsletter-subscription-v1\",\n                            hashedEmail: DYO.dyhash.sha256(document.querySelector(`#email-${widgetId}`).value.toLowerCase()) \/\/ SHA256 encoding of the lowercase email.\n                        }\n                    });\n                };\n                document.querySelector(`#email-${widgetId}`).value = '';\n                document.querySelector(`#first-name-${widgetId}`).value = '';\n                document.querySelector(`#last-name-${widgetId}`).value = '';\n\n                showSuccessfulMessage(response.message)\n            }\n\n            function showSuccessfulMessage(message) {\n                function removeAllChildNodes(parent) {\n                    while (parent.firstChild) {\n                        parent.removeChild(parent.firstChild);\n                    }\n                }\n\n                const container = document.querySelector('.papyrus-newsletter-form-wrapper');\n                removeAllChildNodes(container);\n                container.setAttribute('aria-live', 'assertive');\n                container.innerHTML = '<h2 class=\"newsletter-success-message\">Your email address has been successfully added.<\/h2>';\n\n            }\n\n            document.addEventListener('DOMContentLoaded', function () {\n                let email = document.querySelector(`#email-${widgetId}`);\n                let firstName = document.querySelector(`#first-name-${widgetId}`);\n                let lastName = document.querySelector(`#last-name-${widgetId}`);\n                let newsletterSection = document.querySelector('.papyrus-newsletter-section');\n                let findNewsletterHeading = newsletterSection.querySelector('h1, h2, h3');\n\n                if (findNewsletterHeading) {\n                    newsletterSection.setAttribute('aria-labelledby', findNewsletterHeading.getAttribute('id'));\n                }\n\n                document.querySelector(`#submit-button-${widgetId}`).addEventListener('click', function (e) {\n                    e.preventDefault();\n                    removeValidationError();\n                    isError = false;\n\n                    if (firstName.value.length === 0) {\n                        showValidationErrors(firstName, 'Please enter your first name');\n                        isError = true;\n                    }\n\n                    if (lastName.value.length === 0) {\n                        showValidationErrors(lastName, 'Please enter your last name');\n                        isError = true;\n                    }\n\n                    if (!validateEmail(email.value) && email.value.length !== 0) {\n                        showValidationErrors(email, 'Please enter a valid email address');\n                        isError = true;\n                    }\n\n                    if (email.value.length === 0) {\n                        showValidationErrors(email, 'Please enter your email address');\n                        isError = true;\n                    }\n\n                    if (!isError) {\n                        try {\n                            subscribe({email: email.value, first_name: firstName.value, last_name: lastName.value})\n                        } catch (exception) {\n                        }\n                    } else {\n                        groupErrorMessages()\n                        document.querySelector('.agp-subscription-form .input-error').focus();\n                    }\n                });\n            })\n        }\n\n        subscriptionScript()\n    <\/script>\n<\/div>\n\n\n\n<style>.kb-row-layout-id9119_22edce-d6 > .kt-row-column-wrap{align-content:start;}:where(.kb-row-layout-id9119_22edce-d6 > .kt-row-column-wrap) > .wp-block-kadence-column{justify-content:start;}.kb-row-layout-id9119_22edce-d6 > .kt-row-column-wrap{column-gap:var(--global-kb-gap-md, 2rem);row-gap:var(--global-kb-gap-md, 2rem);padding-top:40px;padding-right:40px;padding-bottom:40px;padding-left:40px;grid-template-columns:minmax(0, 1fr);}.kb-row-layout-id9119_22edce-d6 > .kt-row-layout-overlay{opacity:0.30;}@media all and (max-width: 1024px){.kb-row-layout-id9119_22edce-d6 > .kt-row-column-wrap{padding-top:20px;padding-right:30px;padding-bottom:40px;padding-left:30px;grid-template-columns:minmax(0, 1fr);}}@media all and (max-width: 767px){.kb-row-layout-id9119_22edce-d6 > .kt-row-column-wrap{grid-template-columns:minmax(0, 1fr);}}<\/style><div class=\"kb-row-layout-wrap kb-row-layout-id9119_22edce-d6 alignnone wp-block-kadence-rowlayout\"><div class=\"kt-row-column-wrap kt-has-1-columns kt-row-layout-equal kt-tab-layout-inherit kt-mobile-layout-row kt-row-valign-top\">\n<style>.kadence-column9119_f75c6d-47 > .kt-inside-inner-col,.kadence-column9119_f75c6d-47 > .kt-inside-inner-col:before{border-top-left-radius:0px;border-top-right-radius:0px;border-bottom-right-radius:0px;border-bottom-left-radius:0px;}.kadence-column9119_f75c6d-47 > .kt-inside-inner-col{column-gap:var(--global-kb-gap-sm, 1rem);}.kadence-column9119_f75c6d-47 > .kt-inside-inner-col{flex-direction:column;}.kadence-column9119_f75c6d-47 > .kt-inside-inner-col > .aligncenter{width:100%;}.kadence-column9119_f75c6d-47 > .kt-inside-inner-col:before{opacity:0.3;}.kadence-column9119_f75c6d-47{position:relative;}@media all and (max-width: 1024px){.kadence-column9119_f75c6d-47 > 